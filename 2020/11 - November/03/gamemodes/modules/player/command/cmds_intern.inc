CMD:desbugar(playerid, params[])
{
    if(!IsPlayerIntern(playerid))
        return SendClientMessage(playerid, Erro, "Comando Inválido");

    new id;

    if(sscanf(params, "u", id))
        return SendClientMessage(playerid, Erro, "Digite: /Desbugar [id/nome]");

    if(id == INVALID_PLAYER_ID || !PlayerInfo[id][Logado])
        return SendClientMessage(playerid, Erro, "Jogador(a) não conectado(a)");

    if(playerid == id)
        return SendClientMessage(playerid, Erro, "Você não pode efetuar comando em sí mesmo");

    if(IsArena[id])
        return SendClientMessage(playerid, Erro, "Jogador(a) está na Arena");

    if(PlayerInfo[id][frozen]) PlayerInfo[id][frozen] = false;

    TogglePlayerControllable(id, true);
    SetPlayerVirtualWorld(id, WORLD_DEFAULT);
    SetPlayerInterior(id, 0);
    ClearAnimations(id);

    format(String, MAX_STRING, "VRC-Admin ~ Jogador(a) %s[%d] desbugado(a)", PlayerInfo[id][Nome], id);
    SendClientMessage(playerid, adm, String);
    return 1;
}
